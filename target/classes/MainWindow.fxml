<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="1000.0" prefWidth="2200.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.c0desurfer.kbsah.MainWindow">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <MenuBar >
            <Menu mnemonicParsing="false" text="File">
                <MenuItem mnemonicParsing="false" onAction="#openNewClusterDialog" text="New Cluster" />
                <MenuItem mnemonicParsing="false" onAction="#openPreferences" text="Preferences" />
                <MenuItem mnemonicParsing="false" onAction="#closeApplication" text="Close" />
            </Menu>
            <Menu mnemonicParsing="false" text="View">
                <MenuItem mnemonicParsing="false" fx:id="reopenUtilitiesView" onAction="#reopenUtilitiesView" text="Open Utilities" />
            </Menu>
        </MenuBar>
      <SplitPane fx:id="splitPane" dividerPositions="0.8" orientation="VERTICAL" VBox.vgrow="ALWAYS">
              <AnchorPane fx:id="contentPane" />
          <AnchorPane fx:id="utilitiesPane" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" />
      </SplitPane>
    </VBox>
</AnchorPane>
